def leap_years(n):
    current_years = 0
    start_year = 2021
    while current_years<n:
        if is_leap(start_year):
            current_years +=1
            print(start_year)
        start_year +=1

def is_leap(year):
    return (year%4 == 0 and not year%100 == 0) or year%400==0


leap_years(1)



